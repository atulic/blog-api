{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "apollo-boost": "^0.3.1",
    "graphql": "~14.2.1",
    "react": "^16.12.0",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.12.0",
    "react-scripts": "3.0.1",
    "typeface-roboto": "0.0.54",
    "typescript": "3.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gql-fetch-schema": "apollo schema:download --endpoint=http://localhost:4000/graphql graphql-schema.json",
    "gql-typegen": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/types/graphql-global-types.ts types"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@material-ui/icons": "^4.5.1",
    "@types/enzyme": "^3.10.4",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "apollo": "^2.21.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "prettier": "^1.19.1"
